{% extends '::index.html.twig' %}

{% block body -%}
    <h1>MessagesMo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Message</th>
                <td>{{ entity.message }}</td>
            </tr>
            <tr>
                <th>Sourceaddress</th>
                <td>{{ entity.sourceAddress }}</td>
            </tr>
            <tr>
                <th>Receivedtime</th>
                <td>{{ entity.receivedTime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('messagesmo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('messagesmo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
