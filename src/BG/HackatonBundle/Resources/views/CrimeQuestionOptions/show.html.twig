{% extends '::index.html.twig' %}

{% block body -%}
    <h1>CrimeQuestionOptions</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Option</th>
                <td>{{ entity.option }}</td>
            </tr>
            <tr>
                <th>Optionnumber</th>
                <td>{{ entity.optionNumber }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('crimequestionoptions') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('crimequestionoptions_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
